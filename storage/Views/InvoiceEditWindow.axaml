<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        xmlns:vm="clr-namespace:storage.ViewModels"
        x:Class="storage.Views.InvoiceEditWindow"
        Title="InvoiceEditWindow"
        x:DataType="vm:InvoiceEditWindowModel">
    <StackPanel>
        <TextBox Text="{Binding Id, Mode=TwoWay}" Padding="20" Watermark="ID" IsReadOnly="True" />
        <DatePicker SelectedDate="{Binding CreatedAt, Mode=TwoWay}" Padding="20" />
    </StackPanel>
</Window>
