<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        xmlns:vm="clr-namespace:storage.ViewModels"
        x:Class="storage.Views.InvoiceEditWindow"
        Title="InvoiceEditWindow"
        x:DataType="vm:InvoiceEditWindowModel">
    <StackPanel>
        <DatePicker SelectedDate="{Binding CreatedAt, Mode=TwoWay}" Padding="20" />
		<TimePicker SelectedTime="{Binding CreatedAtTime, Mode=TwoWay}" ClockIdentifier="24HourClock" MinuteIncrement="1" Padding="20"/>
		<Button Content="Save" Padding="20" Click="Save_OnClick"/>
    </StackPanel>
</Window>
